pipeline 
{
   stages 
   {
        stage('dev') {
            steps {
                echo 'application is dev'
            }
        }
         stage('testing') {
            steps {
                echo 'automation testing'
            }
        }
         stage('uat') {
            steps {
                echo 'waiting for user accepatence testing'
            }
        }
         stage('release') {
            steps {
                echo 'application is ready to release to the production service'
            }
        }
    }
}
